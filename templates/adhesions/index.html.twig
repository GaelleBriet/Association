{% extends 'layouts/base.html.twig' %}

{% block title 'Ajouter une adhésion' %}

{% block body %}

  <div class="container my-5 h-100">
    <div class="row">
      <div class="col-md-6 mx-auto">

        <h1 class="text-center mb-5">Ajout d'une adhésion</h1>

        {{ form_start(form) }}

            {{ form_row(form.starting_date) }}
            {{ form_row(form.ending_date) }}

          <div class="d-inline">
            <input type="submit" value="Ajouter une adhésion" class="mb-3 btn btn-primary me-3 col-12" formnovalidate="formnovalidate">
          </div>
          
        {{ form_end(form) }}
          

      </div>
    </div>
  </div>

{% endblock %}