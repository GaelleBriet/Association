<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Association Aide Cavaillonnaise Aux Animaux{% endblock %}</title>

        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('images/empreinte-de-patte.ico') }}">

        {% block stylesheets %}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
        <link rel="stylesheet" href="https://unpkg.com/bootstrap-table@1.21.0/dist/bootstrap-table.min.css">
            {# <link rel="stylesheet" href="{{ asset('build/app.css') }}"> #}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>

    <body class="d-flex flex-column vh-100">
        {{ include('layouts/partials/_flash_messages.html.twig') }}
        {{ include('layouts/partials/_nav.html.twig') }}

        <main>
            {% block body %}
            {% endblock %}
        </main>

        {{ include('layouts/partials/_footer.html.twig') }}


        {% block javascripts %}
            <script src="https://cdn.jsdelivr.net/npm/jquery/dist/jquery.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        <script src="https://unpkg.com/bootstrap-table@1.21.0/dist/bootstrap-table.min.js"></script>
        <script src="https://unpkg.com/bootstrap-table@1.16.0/dist/locale/bootstrap-table-fr-FR.min.js"></script>
            {# <script src="https://unpkg.com/bootstrap-table@1.21.0/dist/bootstrap-table.min.js" defer></script> #}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
