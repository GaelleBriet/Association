<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Association Aide Cavaillonnaise Aux Animaux{% endblock %}</title>

        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('build/images/empreinte-de-patte.ico') }}">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
            {# <link rel="stylesheet" href="{{ asset('build/app.css') }}"> #}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>

    <body>
        {{ include('layouts/partials/_nav.html.twig') }}

        <main role="main" class="container">
            {% block body %}
            {% endblock %}
        </main>

        {{ include('layouts/partials/_footer.html.twig') }}


        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
